templates {
  // Empty template
  empty [
    template { id, name }
  ]
  
  // Single row
  single [
    template { id, name }
    1, "Alice"
  ]
  
  // Template with map
  withMap [
    template { id, value }
    map (item) = {
      id = item.id * 10
      value = item.value + "!"
      extra = true
    }
    1, "test"
    2, "demo"
  ]
}